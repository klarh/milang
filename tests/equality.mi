/* Structural equality tests */

/* integers */
eq_int = 42 == 42           /* expect: 1 */
neq_int = 42 /= 43          /* expect: 1 */
eq_int_f = 1 == 2           /* expect: 0 */

/* strings */
eq_str = "hello" == "hello"  /* expect: 1 */
neq_str = "a" /= "b"        /* expect: 1 */
eq_str_f = "x" == "y"       /* expect: 0 */

/* floats */
eq_float = 3.14 == 3.14     /* expect: 1 */
neq_float = 1.0 /= 2.0     /* expect: 1 */

/* lists */
eq_list = [1, 2, 3] == [1, 2, 3]    /* expect: 1 */
neq_list = [1, 2] /= [1, 3]         /* expect: 1 */
eq_list_len = [1] == [1, 2]         /* expect: 0 */
eq_list_empty = [] == []             /* expect: 1 */

/* nested lists */
eq_nested = [[1, 2], [3]] == [[1, 2], [3]]  /* expect: 1 */

/* records (tagged) */
Shape = { Circle r; Rect w h }
eq_rec = (Circle 5) == (Circle 5)     /* expect: 1 */
neq_rec = (Circle 5) /= (Rect 3 4)    /* expect: 1 */
eq_rec_f = (Circle 5) == (Circle 6)   /* expect: 0 */

/* records with different tags */
diff_tag = (Circle 5) == (Rect 5 5)   /* expect: 0 */

/* list of records */
eq_list_rec = [Circle 1, Circle 2] == [Circle 1, Circle 2]  /* expect: 1 */

/* record containing list */
Container = { Wrapper items }
eq_rec_list = (Wrapper [1, 2]) == (Wrapper [1, 2])  /* expect: 1 */
