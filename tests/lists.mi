-- Test list literals and built-in functions
xs = [1, 2, 3, 4, 5]
main1 = println xs

-- len
main2 = println (len xs)

-- get (indexing)
main3 = println (get xs 0)
main4 = println (get xs 4)

-- push
ys = push xs 6
main5 = println ys

-- concat
zs = concat [10, 20] [30, 40]
main6 = println zs

-- map
doubled = map (\x -> x * 2) xs
main7 = println doubled

-- filter
evens = filter (\x -> x == 2) [1, 2, 3, 4]
main8 = println evens

-- fold
total = fold (\acc x -> acc + x) 0 xs
main9 = println total

-- pattern matching
first = xs ->
  [a, b, ...rest] = println (a + b)
main10 = first

-- empty list
empty = []
main11 = println empty
